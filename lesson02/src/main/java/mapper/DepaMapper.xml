<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.DepaMapper">

    <!--Department getDepartmentByDid(Integer did)-->
    <select id="getDepartmentByDid"  resultType="pojo.Department">
        select  * from mybatis.department where did = #{did}
    </select>
</mapper>